{
  "name": "cdk-webapp-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidtucker/cdk-webapp-tools.git"
  },
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "test": "tsc && jest",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "cdk": "cdk",
    "package": "jsii-pacmak"
  },
  "author": {
    "name": "David Tucker",
    "url": "https://davidtucker.net/",
    "organization": false
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.68.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsii": "^5.3.31",
    "jsii-diff": "^1.96.0",
    "jsii-docgen": "^10.3.25",
    "jsii-pacmak": "^1.96.0",
    "json-schema": "^0.4.0",
    "standard-version": "^9",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.3",
    "constructs": "^10.3.0",
    "aws-cdk-lib": "2.133.0"
  },
  "peerDependencies": {
    "constructs": "^10.0.0",
    "aws-cdk-lib": "^2.133.0"
  },
  "dependencies": {},
  "bundledDependencies": [],
  "keywords": [
    "angular",
    "aws",
    "cdk",
    "config",
    "deploy",
    "react",
    "svelte"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "main": "lib/index.js",
  "license": "MIT",
  "version": "2.49.3",
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {},
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    },
    "excludeTypescript": [
      "src/lambda"
    ]
  },
  "awscdkio": {
    "twitter": "_davidtucker_"
  }
}
